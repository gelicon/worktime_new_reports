            <xml-property name="queryText"><![CDATA[SELECT U.proguser_id ,
       U.proguser_name ,
       U.PROGUSER_FULLNAME ,
       C.capcode_name AS status_name,
       CASE U.proguser_type
         WHEN 1 THEN 'Администратор'
         ELSE 'Пользователь'
       END proguser_type_text
FROM   proguser U,
       capcode C
WHERE  U.proguser_id = ?
  AND  C.capcode_id = U.proguser_status_id
ORDER BY U.proguser_name ]]></xml-property>
